<?xml version="1.0" encoding="UTF-8"?>
<root>
	<Hotkeys>
		<Hotkey type="process" name="Snipping Tool">
			<Key mod="AltShiftWin">PrintScreen</Key>
			<Path>C:\WINDOWS\system32\SnippingTool.exe</Path>
			<Args></Args>
		</Hotkey>
		<Hotkey type="shell" name="Process Document">
			<Key mod="AltWin">C</Key>
			<Path>C:\Important Document.docx</Path>
			<Args></Args>
		</Hotkey>
		<Hotkey type="process" name="Open Clipboard in Explorer">
			<Key mod="AltWin">E</Key>
			<Path>C:\WINDOWS\explorer.exe</Path>
			<Args>{clipboard}</Args>
		</Hotkey>
		<Hotkey type="process" name="Invoke Robocopy">
			<Key mod="AltWin">L</Key>
			<Path>robocopy</Path>
			<Args>{source} {destination}</Args>
			<Prompts>
				<P key="{source}">What is the source?</P>
				<P key="{source}">What is the destination?</P>
			</Prompts>
		</Hotkey>
		<Hotkey type="chord" name="PowerShell or Command Prompt">
			<Key mod="AltCtrlWin">S</Key>
			<Chords>
				<Chord type="process" name="Cmd">
					<Key mod="AltCtrl">C</Key>
					<Path>C:\Windows\System32\cmd.exe</Path>
				</Chord>
				<Chord type="process" name="PowerShell">
					<Key mod="AltCtrl">P</Key>
					<Path>C:\Windows\System32\WindowsPowerShell\v1.0\powershell.exe</Path>
				</Chord>
			</Chords>
		</Hotkey>
	</Hotkeys>
</root>